<%= simple_form_for @user_session, :html => { :class => 'form-horizontal' } do |f| %>
  <fieldset>
    <legend>Login</legend>

	<%= f.input :username %>
	<%= f.input :password %>

    <div class="form-actions">
      <%= f.submit 'Login', :class => 'btn btn-primary' %>
    </div>
  </fieldset>
<% end %>


